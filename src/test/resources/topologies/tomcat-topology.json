{
  "id": "topologyId",
  "node_templates": {
    "Tomcat": {
      "type": "fastconnect.nodes.TomcatGroovy",
      "relationships": {
        "hosted_on": {
          "type": "tosca.relationships.HostedOn",
          "target": "ComputeTomcat"
        }
      }
    },"ComputeTomcat": {
      "type": "tosca.nodes.Compute",
      "properties": {
        "mem_size": 1600,
        "os_arch": "x86_64",
        "os_type": "linux",
        "os_distribution":"ubuntu",
        "os_version":"ubuntu"
      }
    }
  },
  "dependencies": [
  	{
		"name": "tosca-base-types",
		"version": "1.0"
	},
	{
		"name": "apache-lb-types",
		"version": "0.1"
	},
	{
		"name": "tomcatGroovy",
		"version": "0.1"
	}
  ]
}
